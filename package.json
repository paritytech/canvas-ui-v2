{
  "name": "substrate-contracts-explorer",
  "version": "2.0.0",
  "description": "Substrate Contracts Explorer",
  "private": true,
  "type": "module",
  "homepage": "https://paritytech.github.io/substrate-contracts-explorer",
  "scripts": {
    "start": "yarn build && webpack serve --config webpack.dev.cjs --open",
    "build": "NODE_ENV=development webpack --progress --config webpack.dev.cjs",
    "build:prod": "NODE_ENV=production webpack --config webpack.prod.cjs",
    "test:all": "jest --clearCache && node --expose-gc --experimental-vm-modules $(yarn bin)/jest --runInBand",
    "test": "yarn test:all --testPathIgnorePatterns ./__tests__/db/**",
    "test:watch": "yarn test --watch",
    "test:coverage": "yarn test --collectCoverage --coveragePathIgnorePatterns ./src/db/**",
    "test:coverage:all": "yarn test:all --collectCoverage",
    "test:ci": "yarn test:all",
    "lint": "yarn eslint --fix .",
    "format": "yarn prettier --write .",
    "clean": "rm -rf dist",
    "deep-clean": "yarn clean && rm -rf node_modules",
    "regenerate": "yarn deep-clean && yarn && yarn start",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
